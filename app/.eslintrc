{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "@open-wc/eslint-config",
      "prettier",
      "plugin:lit-a11y/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "import/no-extraneous-dependencies": "off",
      "import/no-named-as-default-member": "off",
      "spaced-comment": "off"
    }
}
